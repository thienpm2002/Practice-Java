package Array;

public class RotateArray {
	static void solution(int[] arr, int n) {
		int size = arr.length;
		if (n % size == 0) {
			for (int i = 0; i < size; i++) {
				System.out.print(arr[i] + " ");
			}
		} else {
			if (n > size)
				n = n % size;
			for (int i = size - n; i < size; i++) {
				System.out.print(arr[i] + " ");
			}
			for (int i = 0; i < size - n; i++) {
				System.out.print(arr[i] + " ");
			}

		}

	}

	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 4, 5, 6 };
		int d = 2;
		solution(arr, d);
	}
}
