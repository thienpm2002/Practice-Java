package EasyProblems;

public class DivisibilityByElevent {
	static boolean solution(String n) {
		if (n.length() <= 1 && (n.charAt(0) - '0') != 0)
			return false;
		int sumChan = 0, sumLe = 0;
		for (int i = 0; i < n.length(); i++) {
			int a = (n.charAt(i) - '0');
			if (i % 2 == 0)
				sumChan += a;
			else
				sumLe += a;
		}
		return (Math.abs(sumLe - sumChan) % 11 == 0);
	}

	public static void main(String[] args) {
		String n = "1234567589333892";
		System.out.println(solution(n) ? "True" : "False");
	}
}
