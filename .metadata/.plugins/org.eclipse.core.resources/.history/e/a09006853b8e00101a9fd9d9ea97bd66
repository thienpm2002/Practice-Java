package EasyProblems;

public class CheckPow {
	// Kiem tra x^n = y (n bat ky)
	public static boolean solution(int x, int y) {
		if (y == 1)
			return true;
		while (y != 1) {
			if (y % x == 0) {
				y = y / x;
			} else {
				return false;
			}
		}
		return true;
	}

	public static void main(String[] args) {
		int x = 10;
		int y = 1;

	}
}
