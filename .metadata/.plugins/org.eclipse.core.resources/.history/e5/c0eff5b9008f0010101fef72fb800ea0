package EasyProblems;

public class ArmstrongNumbers {
	static boolean solution(int n) {
		int sum = 0;
		int tmp = n;
		while (tmp != 0) {
			int a = tmp % 10;
			sum = sum + a * a * a;
			tmp = tmp / 10;
		}
		return sum == n;
	}

	public static void main(String[] args) {
		System.out.println(solution(123) ? "True" : "False");
	}
}
