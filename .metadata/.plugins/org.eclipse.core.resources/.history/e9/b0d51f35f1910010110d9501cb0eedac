package Array;

public class AlternateElements {
	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 4, 5, 2 };
		int tmp = 0;
		for (int i = arr.length - 1; i >= 0; i--) {

			if (i == arr.length - 1 || arr[i] > arr[i + 1] && arr[i] > tmp) {
				System.out.print(arr[i] + " ");
				tmp = arr[i];
			} else {
				continue;
			}
		}
	}
}
