package Array;

import java.util.ArrayList;

public class SubArr {
	   static void solution(ArrayList<Integer> arr){
		  
		   int n = arr.size();
		   for(int i =0; i< n; i++) {
			   for(int j =i; j<n; j++) {
				   for(int k = i; k<=j; k++) {
					   System.out.print(arr.get(k) + " ");
				   }
			   }
			   System.out.println();
		   }
		   
	   }
       public static void main(String[] args) {
		
	}
}  
