package EasyProblems;

public class PairCubeCount {
	// Dem co bao nhieu cap (a,b) ma a^3 + b^3 = n, (a,b) khac (b,a);
	static int solution(int n) {
		int result = 0;
		int limit = (int) (Math.cbrt(n));
		for (int a = 1; a <= limit; a++) {
			int a3 = a * a * a;
			int remain = n - a3;
			if (remain <= 0)
				continue;
			int b = (int) Math.round(Math.cbrt(remain));
			if(b>0 && b*b*b + a3 == n) result+=2;
		}
		return result;
	}

	public static void main(String[] args) {
		System.out.println(solution(35));
	}
}
