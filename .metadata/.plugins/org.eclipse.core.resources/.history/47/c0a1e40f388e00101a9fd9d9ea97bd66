package EasyProblems;

public class ReverseNumber {
    // Dao so Vd 12345 -> 54321
	public static int solution(int n) {
		int tmp = 0;
		while (n > 0) {
			tmp = tmp * 10 + n % 10;
			n = n / 10;
		}
		return tmp;
	}

	public static void main(String[] args) {
		int n = 12345;
		System.out.println(solution(n));
	}
}
