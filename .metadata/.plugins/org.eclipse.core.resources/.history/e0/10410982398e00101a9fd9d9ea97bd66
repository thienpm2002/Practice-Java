package EasyProblems;

public class PrimalityTest {
	// Kiem tra so nguyen to
	public static int solution(int n) {
		if (n == 1)
			return 0;
		int mid = n / 2;
		for (int i = 2; i <= mid; i++) {
			if (n % i == 0)
				return 0;
		}
		return 1;
	}

	public static void main(String[] args) {
		int n = 11;
		if (solution(n) == 1) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}
	}
}
