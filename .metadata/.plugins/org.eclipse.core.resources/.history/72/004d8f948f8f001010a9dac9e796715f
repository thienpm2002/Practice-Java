package EasyProblems;

public class DivisibleByFour {
	static boolean solution(String n) {
		if (n.length() == 0)
			return false;
		if (n.length() == 1)
			return (Integer.parseInt(n) % 4 == 0);
		return (Integer.parseInt(n.substring(n.length() - 2)) % 4 == 0);
	}

	public static void main(String[] args) {

	}
}
