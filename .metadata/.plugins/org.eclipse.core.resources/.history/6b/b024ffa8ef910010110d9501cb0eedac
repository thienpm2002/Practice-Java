package Array;

public class AlternateElements {
	public static void main(String[] args) {
		int[] arr = { 10, 20, 30, 40, 50 };
        int tmp =0;
		for (int i = arr.length -2 ; i >= 0; i--) {
			if(arr[i] > arr[i+1] && arr[i] > tmp) {
				System.out.print(arr[i] + " ");
				tmp= arr[i];
			}else {
				continue;
			}
		}
	}
}
