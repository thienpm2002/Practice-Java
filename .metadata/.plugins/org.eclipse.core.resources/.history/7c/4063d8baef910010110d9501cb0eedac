package Array;

public class AlternateElements {
	public static void main(String[] args) {
		int[] arr = { 16, 17, 4, 3, 5, 2 };
		int tmp = 0;
		for (int i = arr.length - 2; i >= 0; i--) {
			if (arr[i] > arr[i + 1] && arr[i] > tmp) {
				System.out.print(arr[i] + " ");
				tmp = arr[i];
			} else {
				continue;
			}
		}
	}
}
