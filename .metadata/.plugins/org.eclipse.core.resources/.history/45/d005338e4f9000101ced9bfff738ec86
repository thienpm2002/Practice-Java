package EasyProblems;

public class NextPermutation {
	static int[] solution(int[] arr) {
		int n = arr.length;
		int pivot = -1;
		for (int i = n - 2; i >= 0; i--) {
			if (arr[i] < arr[i + 1]) {
				pivot = i;
				break;
			}
		}

	}

	public static void main(String[] args) {

	}
}
