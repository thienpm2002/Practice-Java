package EasyProblems;

public class DivisibilityByElevent {
	static boolean solution(String n) {
		if (n.length() <= 1)
			return false;
		int sumChan = 0, sumLe = 0;
		for (int i = 0; i < n.length(); i++) {
			int a = Integer.parseInt(n.charAt(i) + " ");
			if (a % 2 == 0)
				sumChan += a;
			else
				sumLe += a;
		}
		return (Math.abs(sumLe - sumChan) % 4 == 0);
	}

	public static void main(String[] args) {
		String n = "363588395960667043875488";
		System.out.println(solution(n) ? "True" : "False");
	}
}
